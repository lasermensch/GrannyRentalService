@model Projektarbete2.Models.ViewModels.DeleteBookingViewModel;

<div class="row">
    <div class="col-md-5">
        <form asp-action="Create" Method="post">
            <div class="form-group">
                <label class="control-label">Granny</label>
                <input class="form-control" asp-for="@Model.Granny.ID" value="@Model.Granny.FirstName @Model.Granny.LastName" />
                
            </div>
            <div class="form-group">
                <label class="control-label">From</label>
                <input class="form-control" asp-for="Booking.From" value="@Model.Booking.From"/>

            </div>
            <div class="form-group">
                <label class="control-label">To</label>
                <input class="form-control" asp-for="@Model.Booking.To" />

            </div>
            <div class="form-group">
                <input class="btn btn-danger" type="submit" value="Delete Booking" />
            </div>
        </form>
    </div>
</div>